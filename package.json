{
  "name": "lmap",
  "version": "0.0.1",
  "description": "Simply service with chat and map.",
  "main": "src/server",
  "scripts": {
    "start": "node dist/server",
    "dev": "npm run dev:server | npm run dev:client",
    "dev:server": "NODE_ENV=develop babel-node src/server",
    "dev:client": "NODE_ENV=develop webpack-dev-server --progress",
    "build": "NODE_ENV=production npm run build:server | npm run build:client",
    "build:server": "babel src/server -d dist/server",
    "build:client": "babel src/app -d dist/app",
    "test": "mocha --compilers js:babel-core/register --require ./src/test/setup 'test/**/*.@{js|jsx}'",
    "test:server": "echo \"Error: no test specified\" && exit 1",
    "test:client": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "lint:server": "./node_modules/eslint/bin/eslint.js --fix src/server",
    "lint:client": "./node_modules/eslint/bin/eslint.js --fix src/app",
    "test:watch": "npm run test -- --watch-extensions js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batazor/LMap.git"
  },
  "keywords": [
    "react",
    "redux",
    "map",
    "DnD"
  ],
  "author": "batazor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/batazor/LMap/issues"
  },
  "homepage": "https://github.com/batazor/LMap#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.2.0",
    "mocha": "^2.5.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.4.19",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-linkedin": "^1.0.0",
    "passport-twitter": "^1.0.4"
  }
}
