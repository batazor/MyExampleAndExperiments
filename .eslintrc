rules:
  indent:
    - error
    - 2
  quotes:
    - 2
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - always
  no-console: 0
  # React
  no-danger: 0
  react/jsx-uses-vars: 2
env:
  es6: true
  node: true
  browser: true
extends: 'eslint:recommended'
ecmaFeatures:
  jsx: true
  experimentalObjectRestSpread: true
  modules: true
parser: "babel-eslint"
parserOptions:
  sourceType: "module"
plugins:
  - react
