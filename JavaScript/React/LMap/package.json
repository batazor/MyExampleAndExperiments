{
  "name": "lmap",
  "version": "0.0.1",
  "description": "Simply service with chat and map.",
  "main": "src/server",
  "scripts": {
    "start": "node dist/server",
    "dev": "npm run dev:server | npm run dev:client",
    "dev:server": "NODE_ENV=develop babel-node src/server",
    "dev:client": "NODE_ENV=develop webpack-dev-server --progress --inline --hot",
    "docs": "$(npm bin)/esdoc -c esdoc.json",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "NODE_ENV=production babel src/server -d dist/server",
    "build:client": "NODE_ENV=production webpack --progress && npm run build:file",
    "build:file": "mkdir -p ./dist/public/dist && mv ./dist/*.* ./dist/public/ && cp -r ./src/public/* ./dist/public/",
    "test": "npm run test:server && npm run test:client",
    "test:server": "echo \"Error: no test specified\" && exit 1",
    "test:client": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "echo \"Error: no test specified\" && exit 1",
    "lint": "$(npm bin)/eslint.js src && flow",
    "lint:server": "./node_modules/eslint/bin/eslint.js --fix src/server",
    "lint:client": "./node_modules/eslint/bin/eslint.js --fix src/app",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batazor/LMap.git"
  },
  "keywords": [
    "react",
    "redux",
    "map",
    "DnD"
  ],
  "author": "batazor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/batazor/LMap/issues"
  },
  "homepage": "https://github.com/batazor/LMap#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "classnames": "^2.2.6",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.3.1",
    "esdoc": "^1.1.0",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.101.0",
    "graphql": "^0.13.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.7",
    "leaflet": "^0.7.7",
    "mocha": "^2.5.3",
    "node-sass": "^3.13.1",
    "postcss-loader": "^0.9.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^1.3.1",
    "react-leaflet": "^0.11.5",
    "react-redux": "^4.4.10",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools": "^3.5.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.23.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.16.5",
    "whatwg-fetch": "^1.1.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "graphql": "^0.13.2",
    "immutable": "^3.8.2",
    "mongoose": "^5.0.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-linkedin": "^1.0.0",
    "passport-twitter": "^1.0.4"
  }
}
