{
  "name": "lmap",
  "version": "0.0.1",
  "description": "Simply service with chat and map.",
  "main": "src/server",
  "scripts": {
    "start": "node dist/server",
    "dev": "npm run dev:server | npm run dev:client",
    "dev:server": "NODE_ENV=develop babel-node src/server",
    "dev:client": "NODE_ENV=develop webpack-dev-server --progress --inline --hot",
    "docs": "$(npm bin)/esdoc -c esdoc.json",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "NODE_ENV=production babel src/server -d dist/server",
    "build:client": "NODE_ENV=production webpack --progress && npm run build:file",
    "build:file": "mkdir -p ./dist/public/dist && mv ./dist/*.* ./dist/public/ && cp -r ./src/public/* ./dist/public/",
    "test": "npm run test:server && npm run test:client",
    "test:server": "echo \"Error: no test specified\" && exit 1",
    "test:client": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "echo \"Error: no test specified\" && exit 1",
    "lint": "$(npm bin)/eslint.js src && flow",
    "lint:server": "./node_modules/eslint/bin/eslint.js --fix src/server",
    "lint:client": "./node_modules/eslint/bin/eslint.js --fix src/app",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/batazor/LMap.git"
  },
  "keywords": [
    "react",
    "redux",
    "map",
    "DnD"
  ],
  "author": "batazor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/batazor/LMap/issues"
  },
  "homepage": "https://github.com/batazor/LMap#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.11.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.26.0",
    "graphql": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.2.0",
    "json-loader": "^0.5.4",
    "leaflet": "^0.7.7",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-leaflet": "^0.11.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-graphql": "^0.5.3",
    "express-session": "^1.13.0",
    "graphql": "^0.5.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.4.19",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "passport-linkedin": "^1.0.0",
    "passport-twitter": "^1.0.4"
  }
}
